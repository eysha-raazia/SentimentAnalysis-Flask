pipeline {
  agent any {
    stages {
      stage('checkout') {
        steps {
          git branch:'master', url:'https://github.com/eysha-raazia/SentimentAnalysis-Flask.git'
        }
      }
      stage('Build') {
        steps {
          echo 'Builing...!'
          sh 'pip install -r requirements.txt'
        }
      }
    }
  }
}
